{
  "name": "talenet",
  "version": "0.9.0-SNAPSHOT",
  "description": "tale:net - intergalactic skill & idea sharing community",
  "homepage": "https://blog.t4l3.net",
  "repository": {
    "type": "git",
    "url": "https://github.com/talenet/talenet.git"
  },
  "author": "Andreas Baldeau <andreas@baldeau.net>, Henry Bubert <cryptix@riseup.net>, Torben Spieker <torben.spieker@posteo.de>, Kathia von Roth <stephiewunder@posteo.de>, Sascha Wollin <saschawollin@gmail.com>",
  "private": true,
  "main": "./dist/electron/index.js",
  "scripts": {
    "makenix": "node2nix --nodejs-10 -c node2nix-taleEnv.nix --supplement-input node2nix-supplement.json --supplement-output node2nix-supplement.nix",
    "build": "node .electron-vue/build.js && electron-builder",
    "build:win": "node .electron-vue/build.js & electron-builder",
    "build:dir": "node .electron-vue/build.js && electron-builder --dir",
    "build:clean": "cross-env BUILD_TARGET=clean node .electron-vue/build.js",
    "build:web": "cross-env BUILD_TARGET=web node .electron-vue/build.js",
    "e2e": "npm run pack && mocha test/e2e",
    "dev": "node .electron-vue/dev-runner.js",
    "unit": "cross-env BABEL_ENV=test karma start test/unit/karma.conf.js --single-run",
    "test": "true",
    "lint": "eslint --ext .js,.vue -f ./node_modules/eslint-friendly-formatter src test/unit/specs test/e2e/specs",
    "lint:fix": "eslint --ext .js,.vue -f ./node_modules/eslint-friendly-formatter --fix src test/unit/specs test/e2e/specs",
    "pack": "npm run pack:main && npm run pack:renderer",
    "pack:main": "cross-env NODE_ENV=production webpack --display-error-details --progress --colors --config .electron-vue/webpack.main.config.js",
    "pack:renderer": "cross-env NODE_ENV=production webpack --display-error-details --progress --colors --config .electron-vue/webpack.renderer.config.js"
  },
  "build": {
    "remoteBuild": false,
    "npmRebuild": true,
    "productName": "talenet",
    "appId": "net.tale.client",
    "directories": {
      "output": "build"
    },
    "files": [
      "dist/electron/**/*"
    ],
    "dmg": {
      "contents": [
        {
          "x": 410,
          "y": 150,
          "type": "link",
          "path": "/Applications"
        },
        {
          "x": 130,
          "y": 150,
          "type": "file"
        }
      ]
    },
    "mac": {
      "icon": "build/icons/icon.icns"
    },
    "win": {
      "publish": [
        {
          "provider": "github",
          "repo": "talenet",
          "owner": "talenet"
        }
      ],
      "icon": "build/icons/icon.ico"
    },
    "linux": {
      "target": [
        "deb",
        "AppImage"
      ],
      "icon": "build/icons",
      "category": "P2P"
    }
  },
  "dependencies": {
    "async": "^2.6.0",
    "babel-polyfill": "^6.26.0",
    "bluebird": "^3.5.1",
    "bootstrap": "^4.1.3",
    "bootstrap-vue": "^2.0.0-rc.1",
    "chalk": "^2.3.1",
    "d3": "^5.6.0",
    "electron-context-menu": "^0.9.1",
    "electron-default-menu": "^1.0.1",
    "electron-window-state": "^4.1.1",
    "file-saver": "^1.3.3",
    "fix-path": "^2.1.0",
    "graphlib": "^2.1.5",
    "highlight.js": "^9.12.0",
    "lodash": "^4.17.5",
    "open": "0.0.5",
    "prompt": "^1.0.0",
    "pull-abortable": "^4.1.1",
    "pull-cat": "^1.1.11",
    "pull-file-reader": "^1.0.2",
    "pull-paramap": "^1.2.2",
    "pull-sort": "^1.0.1",
    "pull-stream": "^3.6.9",
    "request": "^2.88.0",
    "scuttle-shell": "^0.1.3",
    "ssb-client": "^4.6.0",
    "ssb-keys": "^7.0.16",
    "ssb-marked": "^0.7.3",
    "vee-validate": "^2.0.0-rc.25",
    "vue": "^2.5.17",
    "vue-electron": "^1.0.6",
    "vue-i18n": "^7.3.2",
    "vue-router": "^3.0.1",
    "vue-slider-component": "^2.7.6",
    "vuex": "^3.0.1",
    "vuex-persistedstate": "^2.4.2",

    "autoprefixer": "^8.0.0",
    "avoriaz": "^6.3.0",
    "babel-core": "^6.25.0",
    "babel-eslint": "^8.2.6",
    "babel-loader": "^7.1.1",
    "babel-plugin-istanbul": "^4.1.5",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-stage-0": "^6.24.1",
    "babel-register": "^6.24.1",
    "babili-webpack-plugin": "^0.1.2",
    "cfonts": "^1.1.3",
    "chai": "^4.0.0",
    "copy-webpack-plugin": "^4.4.1",
    "cross-env": "^5.1.1",
    "css-loader": "^0.28.9",
    "del": "^3.0.0",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "^1.1.5",
    "html-webpack-plugin": "^2.30.1",
    "inject-loader": "^3.0.0",
    "jquery": "^3.3.1",
    "mathsass": "^0.10.1",
    "multispinner": "^0.2.1",
    "nightwatch": "^0.9.12",
    "node-loader": "^0.6.0",
    "node-sass": "^4.9.3",
    "require-dir": "^1.0.0",
    "sass-loader": "^6.0.6",
    "semver": "^5.5.1",
    "spectron": "^3.8.0",
    "style-loader": "^0.20.1",
    "url-loader": "^0.6.2",
    "vue-loader": "^14.1.1",
    "vue-style-loader": "^3.1.2",
    "vue-template-compiler": "^2.5.17",
    "webpack": "^3.11.0",
    "webpack-dev-server": "^2.11.3",
    "webpack-hot-middleware": "^2.23.1",
    "webpack-merge": "^4.1.4"
  },
  "engines": {
    "node": ">= 4.0.0",
    "npm": ">= 3.0.0"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ]
}
